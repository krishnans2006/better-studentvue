{
  "manifest_version": 3,
  "name": "Better StudentVUE",
  "description": "Improves the Edupoint Synergy StudentVUE experience",
  "version": "1.0.0.0",
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "content_scripts": [
    {
      "js": ["scripts/gradebook.js"],
      "matches": ["https://sisstudent.fcps.edu/SVUE/PXP2_Gradebook.aspx*"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "storage"
  ]
}